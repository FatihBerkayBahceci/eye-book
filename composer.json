{
    "name": "fatihberkaybahceci/eye-book",
    "description": "Enterprise Appointment Management System for Eye Care Practices",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Fatih Berkay BahÃ§eci",
            "email": "fatih@eyebook.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "brain/monkey": "^2.6",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "EyeBook\\": "includes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EyeBook\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-unit": "phpunit --testsuite=\"Eye-Book Unit Tests\"",
        "test-security": "phpunit --testsuite=\"Eye-Book Security Tests\"",
        "test-hipaa": "phpunit --testsuite=\"Eye-Book HIPAA Compliance Tests\"",
        "test-performance": "phpunit --testsuite=\"Eye-Book Performance Tests\"",
        "test-coverage": "phpunit --coverage-html tests/coverage/html"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}